// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Stage1Pending {
  username   String
  hostedLink String
  email      String @id @unique
}

model Stage1User {
  username   String @id @unique
  hostedLink String
  email      String @unique
  grade      Int    @default(0)
}

model Stage1UserFailed {
  username   String @id @unique
  hostedLink String
  email      String @unique
  grade      Int    @default(0)
}
